{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "api/whois.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/whois/(?<domain>[^/]+)", "dest": "/api/whois?domain=$domain" },
    { "src": "/api/whois", "dest": "/api/whois" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
